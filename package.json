{
  "name": "pixel-gif",
  "version": "0.0.0-alpha.1",
  "description": "Parse Gif to ImageData in cross-platform",

  "files": [
    "index.js",
    "lib"
  ],
  "browser": {
    "./src": false
  },

  "scripts": {
    "start": "jasminetea test --verbose --watch",
    "localhost": "zuul test/api.spec --local 59798 --open",

    "cover": "jasminetea test --verbose --cover --report",
    "test": "npm run cover",
    "posttest": "zuul test/api.spec && echo Perfect posttest || echo Ignored posttest",

    "prepublish": "node -e \"if(process.env.TRAVIS){}else{process.exit(1)}\" && npm run compile || echo skip prepublish",
    "compile": "coffee --bare --compile index.coffee",
    "postcompile": "coffee --output lib --bare --compile src"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover"
    }
  },
  
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "license": "MIT",
  "dependencies": {
    "omggif": "^1.0.7",
    "pixel-util": "0.0.0-beta.2"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^10.2.3",
    "coffeeify": "^1.1.0",
    "fixture-images": "^0.1.3",
    "ghooks": "^0.3.2",
    "jasminetea": "^0.2.0-beta.1",
    "object-parser-cli": "0.0.0",
    "zuul": "^3.0.0"
  },

  "keywords": [
    "pixel",
    "gif"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/59naga/pixel-gif-.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/pixel-gif-/issues"
  },
  "homepage": "https://github.com/59naga/pixel-gif-#readme"
}
